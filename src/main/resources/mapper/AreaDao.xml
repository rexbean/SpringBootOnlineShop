<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rex.onlineShopSpringBoot.dao.AreaDao">
    <select id ="queryArea" resultType="com.rex.onlineShopSpringBoot.entity.Area">
        SELECT area_id, area_name,priority, created_time, modified_time
        FROM tb_area ORDER BY priority DESC
    </select>
</mapper>